$qry = "SELECT location_master.*, location_type_master.name as tname, country_master.name as cname, state_master.name as sname, theme_master.name as thname FROM location_master LEFT JOIN location_type_master ON (location_type_master.id = location_master.type_id) LEFT JOIN country_master ON (country_master.id = location_master.country_id) LEFT JOIN state_master ON (state_master.id = location_master.state_id) LEFT JOIN theme_master ON (theme_master.id = location_master.theme_id)";
